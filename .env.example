# Configurações da API Neo Idea
VITE_API_BASE_URL=https://app.neoidea.com.br/sistema/index.php?r=outlet/services
VITE_MDI_ID=172

# Configurações de autenticação
VITE_AUTH_TOKEN_KEY=@NeoIdea:token
VITE_USER_DATA_KEY=@NeoIdea:user

# Configurações de idioma
VITE_DEFAULT_LANG=pt-BR
VITE_SUPPORTED_LANGS=["pt-BR","en-US","es-ES"]

# Endpoints da API
VITE_ENDPOINT_AUTH=autenticaFaExterno
VITE_ENDPOINT_GET_PROFILE=obterPerfilExterno
VITE_ENDPOINT_UPDATE_PROFILE=gravarPerfilExterno
VITE_ENDPOINT_GET_NEOS=getNeosExterno
VITE_ENDPOINT_LINK_DEVICE=vincularDispositivoExterno
VITE_ENDPOINT_UNLINK_DEVICE=desvincularDispositivoExterno
VITE_ENDPOINT_LINK_INVITE=vincularConviteExterno
VITE_ENDPOINT_MAILING=receberMailingExterno
VITE_ENDPOINT_GET_CAROUSEL=getCarrosselExterno
VITE_ENDPOINT_GET_LAYOUT=getLayoutExterno

# Configurações de criptografia
VITE_CRYPTO_METHOD=sha1

# Configurações de timeout da API (em milissegundos)
VITE_API_TIMEOUT=30000

# Configurações de paginação
VITE_DEFAULT_PAGE_SIZE=10

# Configurações de cache
VITE_CACHE_DURATION=3600000

# Configurações de mídia
VITE_MEDIA_TYPES=["video","podcast","documento","conteudolivre"]

# Configurações de perfil
VITE_PROFILE_GENDER_OPTIONS=["masculino","feminino"]